{
    "manifest_version": 3,
    "name": "Example Extension",
    "version": "1.0",
    "permissions": [
      "scripting",
      "activeTab",
      "webNavigation"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "host_permissions": [
      "http://www.example.com/*",
      "https://www.example.com/*"
    ]
  }
  